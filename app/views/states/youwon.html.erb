<div class="tts-apps">
		<h1>You did it!</h1>
		<h3>You named all 50 States!</h3>

		<p>Here they are:</p>

		<div>
			<% i = 1 %>
			<% @states.each do |state| %>
				<%= "#{state.id}. #{state.name.capitalize}" %><br />
				<% i += 1 %>
			<% end %>
		</div>

		<div class="martop-20">
				<%= link_to "Take Another Quiz", quizzes_path %>
		</div>
</div>

<script>
document.getElementById("topbar").style.display = "none";
</script>